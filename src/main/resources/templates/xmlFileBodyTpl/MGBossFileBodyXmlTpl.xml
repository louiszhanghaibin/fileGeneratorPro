<template>
	<filter>
		<type>sep</type>
		<startWith></startWith>
		<seperator>|</seperator>
		<fixLength>0</fixLength>
		<fixRowNum>0</fixRowNum>
		<fields>
			<field>
				<name>TRANSACTION_SERIAL_NO</name>
				<regx>^\w{1,32}$</regx>
				<isUnique>yes</isUnique>
				<length>9</length>
				<component>
					<fields>
						<field>
							<name>1</name>
							<fixValue>1099920151012111111</fixValue>
						</field>
						<field>
							<name>2</name>
							<isUnique>yes</isUnique>
							<length>9</length>
							<regx>^\w{1,10}$</regx>
						</field>
					</fields>
				</component>
			</field>
			<field>
				<name>BUSI_SERIAL_NO</name>
				<regx>^\w{1,32}$</regx>
				<isUnique>yes</isUnique>
				<length>9</length>
			</field>
			<field>
				<name>PAYMENT_SERIAL_NO</name>
				<regx>^\w{0,32}$</regx>
				<isUnique>yes</isUnique>
				<length>9</length>
			</field>
			<field>
				<name>SETTLE_DATE</name>
				<fromInput>settleDate</fromInput>
				<regx>^(2([1-9]\d{2}|01[5-9]|0[2-9]\d)|[3-9]\d{3})(((01|03|05|07|08|10|12)(0[1-9]|[1-2]\d|3[0-1]))|((04|06|09|11)(0[1-9]|[1-2]\d|30))|(02(0[1-9]|[1-2]\d)))$</regx>
			</field>
			<field>
				<name>RECORD_TYPE</name>
				<fixValue>01</fixValue>
				<regx>^\w{2}$</regx>
			</field>
			<field>
				<name>TRADE_TYPE</name>
				<fixValue>01</fixValue>
				<regx>^\w{2}$</regx>
			</field>
			<field>
				<name>INITIATOR_ORG_ID</name>
				<fixValue>0064</fixValue>
				<regx>^0064$</regx>
			</field>
			<field>
				<name>USER_ID_TYPE</name>
				<fixValue>00</fixValue>
				<regx>^\w{2}$</regx>
			</field>
			<field>
				<name>USER_ID_NO</name>
				<regx>^\w{1,32}$</regx>
				<fixValue>13800000000</fixValue>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>BOSS_RECHARGE_AMOUNT</name>
				<regx>^0|[1-9]\d{0,8}$</regx>
				<length>6</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>CHANNEL_ID</name>
				<regx>^\w{2}$</regx>
				<fixValue>00</fixValue>
			</field>
			<field>
				<name>PAY_AIM_TYPE</name>
				<regx>^01|02$</regx>
				<fixValue>01</fixValue>
			</field>
			<field>
				<name>ORDER_NO</name>
				<regx>^\w{1,32}$</regx>
				<component>
					<fields>
						<field>
							<name>1</name>
							<fixValue>0064</fixValue>
						</field>
						<field>
							<name>2</name>
							<fromInput>settleDate</fromInput>
						</field>
						<field>
							<name>3</name>
							<fixValue>000000</fixValue>
						</field>
						<field>
							<name>4</name>
							<fromInput>province</fromInput>
						</field>
						<field>
							<name>5</name>
							<length>11</length>
							<regx>^\w{11}$</regx>
							<isUnique>yes</isUnique>
						</field>
					</fields>
				</component>
			</field>
			<field>
				<name>PRODUCT_NO</name>
				<regx>^.*{0,32}$</regx>
				<length>3</length>
			</field>
			<field>
				<name>ORDER_AMOUNT</name>
				<regx>^[1-9]\d{0,8}$</regx>
				<length>3</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>SETTLEMENT_AMOUNT</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<length>3</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>ORDER_QUANTITY</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<length>3</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>ACTIVITY_NO</name>
				<regx>^\w{0,32}$</regx>
				<length>3</length>
			</field>
			<field>
				<name>PRODUCT_SHELF_NO</name>
				<regx>^\w{0,32}$</regx>
				<length>3</length>
			</field>
			<field>
				<name>THIRDPAY_SERVICE_CHARGE</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<length>3</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>UPAY_SERVICE_CHARGE</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<length>3</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>INTEGRAL_FEE</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<length>3</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>CREDIT_CARD_FEE</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<length>3</length>
				<isNumOnly>yes</isNumOnly>
			</field>
			<field>
				<name>TRADE_STATUS</name>
				<regx>^\w{1}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>OTHER_CHARGE1</name>
				<regx>^\w{0,9}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>OTHER_CHARGE2</name>
				<regx>^\w{0,9}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>OTHER_CHARGE3</name>
				<regx>^\w{0,9}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>OTHER_CHARGE4</name>
				<regx>^\w{0,9}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>OTHER_CHARGE5</name>
				<regx>^\w{0,9}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>RETURN_CODE</name>
				<regx>^\w{0,15}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>RED_PACKETS_AMOUNT</name>
				<regx>^\w{0,5}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>REMARK2</name>
				<regx>^\w{0,5}$</regx>
				<fixValue>0</fixValue>
			</field>
			<field>
				<name>REMARK3</name>
				<regx>^\w{0,5}$</regx>
				<fixValue>0</fixValue>
				<isEnd>yes</isEnd>
			</field>
		</fields>
	</filter>
</template>