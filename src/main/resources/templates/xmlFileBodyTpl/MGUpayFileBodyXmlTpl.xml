<template>
	<filter>
		<type>sep</type>
		<startWith>0064</startWith>
		<seperator>|</seperator>
		<fixRowNum>0</fixRowNum>
		<fixLength>0</fixLength>
		<fields>
			<field>
				<name>MERCHANT_TYPE</name>
				<regx>^0064$</regx>
				<fixValue>0064</fixValue>
			</field>
			<field>
				<name>ORDER_NO</name>
				<regx>^\w{1,32}$</regx>
				<component>
					<fields>
						<field>
							<name>1</name>
							<fixValue>0064</fixValue>
						</field>
						<field>
							<name>2</name>
							<fromInput>settleDate</fromInput>
						</field>
						<field>
							<name>3</name>
							<fixValue>000000</fixValue>
						</field>
						<field>
							<name>4</name>
							<fromInput>province</fromInput>
						</field>
						<field>
							<name>5</name>
							<length>11</length>
							<regx>^\w{11}$</regx>
							<isUnique>yes</isUnique>
						</field>
					</fields>
				</component>
			</field>
			<field>
				<name>TRADE_TIME</name>
				<fixValue>20151012010101</fixValue>
				<regx>^(2([1-9]\d{2}|01[5-9]|0[2-9]\d)|[3-9]\d{3})(((01|03|05|07|08|10|12)(0[1-9]|[1-2]\d|3[0-1]))|((04|06|09|11)(0[1-9]|[1-2]\d|30))|(02(0[1-9]|[1-2]\d)))([0-1]\d|2[0-3])([0-5]\d)([0-5]\d)$</regx>
			</field>
			<field>
				<name>ORDER_TYPE</name>
				<fixValue>01</fixValue>
				<regx>^01|02$</regx>
			</field>
			<field>
				<name>USER_PAY_AMOUNT</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<fixValue>100</fixValue>
			</field>
			<field>
				<name>RED_PACKETS_AMOUNT</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<fixValue>99</fixValue>
			</field>
			<field>
				<name>ORDER_AMOUNT</name>
				<regx>^[1-9]\d{0,8}$</regx>
				<fixValue>1</fixValue>
			</field>
			<field>
				<name>BOSS_RECHARGE_AMOUNT</name>
				<regx>^0|[1-9]\d{0,8}$</regx>
				<fixValue>99</fixValue>
			</field>
			<field>
				<name>PHONE_NUMBER</name>
				<regx>^\d{11}$</regx>
				<fixValue>13800000000</fixValue>
			</field>
			<field>
				<name>PROVINCE</name>
				<fromInput>province</fromInput>
				<regx>^100|471|220|531|311|351|551|210|250|571|591|898|200|771|971|270|731|791|371|891|280|230|290|851|871|931|951|991|431|240|451$</regx>
			</field>
			<field>
				<name>PRODUCT_NAME</name>
				<fixValue>话费充值</fixValue>
				<regx>^([\u4e00-\u9fa5]{0,256})|(\w{0,256})$</regx>
			</field>
			<field>
				<name>PRODUCT_QUANTITY</name>
				<fixValue>1</fixValue>
				<regx>^[1-9]\d{0,8}|\s*$</regx>
			</field>
			<field>
				<name>PRODUCT_NO</name>
				<fixValue>aaa111</fixValue>
				<regx>^\w{0,32}$</regx>
			</field>
			<field>
				<name>DISCOUNT</name>
				<fixValue>1</fixValue>
				<regx>^1|1.0000000|0.0000000|0.\d{0,7}|\s*$</regx>
			</field>
			<field>
				<name>ACTIVITY_NO</name>
				<fixValue>aaa111</fixValue>
				<regx>^\w{0,32}$</regx>
			</field>
			<field>
				<name>THIRD_PAYMENT_ID</name>
				<fixValue>0058</fixValue>
				<regx>^\w{4}|\s$</regx>
			</field>
			<field>
				<name>SETTLEDATE</name>
				<fromInput>settleDate</fromInput>
				<regx>^(2([1-9]\d{2}|01[5-9]|0[2-9]\d)|[3-9]\d{3})(((01|03|05|07|08|10|12)(0[1-9]|[1-2]\d|3[0-1]))|((04|06|09|11)(0[1-9]|[1-2]\d|30))|(02(0[1-9]|[1-2]\d)))$</regx>
			</field>
			<field>
				<name>PAYMENT_TYPE</name>
				<fixValue>01</fixValue>
				<regx>^01|02|03$</regx>
			</field>
			<field>
				<name>TRANSACTION_SERIAL_NO</name>
				<regx>^\w{1,32}$</regx>
				<length>20</length>
				<isUnique>yes</isUnique>
			</field>
			<field>
				<name>USER_TYPE</name>
				<regx>^01|05$</regx>
				<fixValue>01</fixValue>
			</field>
			<field>
				<name>USER_ID</name>
				<regx>^\w{0,32}$</regx>
				<fixValue>1234567890</fixValue>
			</field>
			<field>
				<name>MERVAR</name>
				<regx>^\w{0,64}$</regx>
				<fixValue>1234567890</fixValue>
			</field>
			<field>
				<name>CURRENCY</name>
				<regx>^\w{1,20}$</regx>
				<fixValue>RMB</fixValue>
			</field>
			<field>
				<name>UPAY_SERVICE_CHARGE</name>
				<regx>^0|[1-9]\d{0,8}|\s*$</regx>
				<fixValue>3</fixValue>
			</field>
			<field>
				<name>UPAY_ALIPAY_ACCOUNT</name>
				<regx>^.{0,20}$</regx>
				<fixValue>louiszhang</fixValue>
			</field>
			<field>
				<name>TRADE_CLOSING_TIME</name>
				<regx>^\d{1,10}$</regx>
				<fixValue>1010</fixValue>
			</field>
			<field>
				<name>TRADE_STATUS</name>
				<regx>^0|1$</regx>
				<fixValue>1</fixValue>
			</field>
			<field>
				<name>BUSI_SERIAL_NO</name>
				<regx>^\w{1,32}$</regx>
				<length>20</length>
				<isUnique>yes</isUnique>
				<isEnd>yes</isEnd>
			</field>
		</fields>
	</filter>
</template>