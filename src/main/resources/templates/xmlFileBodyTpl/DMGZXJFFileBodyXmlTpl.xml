							<fromInput>province</fromInput>
							<regx>^\w{3}$</regx>
						</field>
						<field>
							<name>copn2</name>
							<fromInput>settleDate</fromInput>
							<regx>^\d{8}</regx>
						</field>
						<field>
							<name>copn3</name>
							<regx>^.{1,9}$</regx>
