<template>
	<filter>
		<type>sep</type>
		<startWith></startWith>
		<fixRowNum>0</fixRowNum>
		<seperator>|</seperator>
		<fixLength>0</fixLength>
		<fields>
			<field>
				<name>orderNoPayment</name>
				<regx>^\w{1,64}$</regx>
				<length>20</length>
				<isUnique>yes</isUnique>
			</field>
			<field>
				<name>orderNoUpay</name>
				<regx>^0064(\d{28})$</regx>
				<component>
					<fields>
						<field>
							<name>1</name>
							<fixValue>0064</fixValue>
						</field>
						<field>
							<name>2</name>
							<fromInput>settleDate</fromInput>
						</field>
						<field>
							<name>3</name>
							<fixValue>000000</fixValue>
						</field>
						<field>
							<name>4</name>
							<fromInput>province</fromInput>
						</field>
						<field>
							<name>5</name>
							<length>11</length>
							<regx>^\w{11}$</regx>
							<isUnique>yes</isUnique>
						</field>
					</fields>
				</component>
			</field>
			<field>
				<name>userPayAmount</name>
				<regx>^\d{0,9}|\s*$</regx>
				<isNumOnly>yes</isNumOnly>
				<length>6</length>
			</field>
			<field>
				<name>alipayServiceCharge</name>
				<regx>^\d{0,9}|\s*$</regx>
				<isNumOnly>yes</isNumOnly>
				<length>6</length>
			</field>
			<field>
				<name>alipayServiceChargeRate</name>
				<regx>^0\.\d{1,}|\s*$</regx>
				<fixValue>0.003</fixValue>
			</field>
			<field>
				<name>merchantType</name>
				<fixValue>0064</fixValue>
				<regx>^0064$</regx>
			</field>
			<field>
				<name>province</name>
				<fromInput>province</fromInput>
				<regx>^100|471|220|531|311|351|551|210|250|571|591|898|200|771|971|270|731|791|371|891|280|230|290|851|871|931|951|991|431|240|451$</regx>
			</field>
			<field>
				<name>settleDate</name>
				<fromInput>settleDate</fromInput>
				<regx>^(2([1-9]\d{2}|01[5-9]|0[2-9]\d)|[3-9]\d{3})(((01|03|05|07|08|10|12)(0[1-9]|[1-2]\d|3[0-1]))|((04|06|09|11)(0[1-9]|[1-2]\d|30))|(02(0[1-9]|[1-2]\d)))$</regx>
			</field>
			<field>
				<name>tradeTime</name>
				<fixValue>20151012010101</fixValue>
				<regx>^(2([1-9]\d{2}|01[5-9]|0[2-9]\d)|[3-9]\d{3})(((01|03|05|07|08|10|12)(0[1-9]|[1-2]\d|3[0-1]))|((04|06|09|11)(0[1-9]|[1-2]\d|30))|(02(0[1-9]|[1-2]\d)))([0-1]\d|2[0-3])([0-5]\d)([0-5]\d)$</regx>
			</field>
			<field>
				<name>alipayTradingId</name>
				<regx>^\w{0,64}$</regx>
				<length>20</length>
				<isUnique>yes</isUnique>
			</field>
			<field>
				<name>transactionType</name>
				<regx>^\d{1}$</regx>
				<fixValue>1</fixValue>
			</field>
			<field>
				<name>isRefound</name>
				<regx>^0|1$</regx>
				<fixValue>1</fixValue>
				<isEnd>yes</isEnd>
			</field>
		</fields>
	</filter>
</template>